{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "linebreak-style": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "object-curly-newline": ["error", { "consistent": true }]
  },
  "settings": {
    "import/resolver": "webpack"
  },
}
